#include <stdio.h>

int is_prime(int n) {
    if (n <= 1) {
        return 0;
    }
    for (int i = 2; i * i <= n; i++) {
        if (n % i == 0) {
            return 0;
        }
    }
    return 1;
}

int count_primes(int start, int end) {
    int count = 0;
    for (int i = start; i <= end; i++) {
        if (is_prime(i)) {
            count++;
        }
    }
    return count;
}

int main() {
    int start, end;
    printf("Enter start and end of range: ");
    scanf("%d %d", &start, &end);
    printf("Number of primes between %d and %d: %d\n", start, end, count_primes(start, end));
    return 0;
}
